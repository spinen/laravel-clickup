{
    "name": "spinen/laravel-clickup",
    "description": "SPINEN's Laravel Package for ClickUp.",
    "keywords": [
        "clickup",
        "client",
        "laravel",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        },
        {
            "name": "Stephen Finney",
            "email": "stephen.finney@spinen.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^9.19",
        "nesbot/carbon": "^2.62.1"
    },
    "require-dev": {
        "laravel/pint": "^1.4",
        "mockery/mockery": "^1.5.1",
        "phpunit/phpunit": "^9.5.8",
        "psy/psysh": "^0.11",
        "scrutinizer/ocular": "^1.9",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\ClickUp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\ClickUp\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\ClickUp\\Providers\\ClientServiceProvider",
                "Spinen\\ClickUp\\Providers\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
