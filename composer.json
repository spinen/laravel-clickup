{
    "name": "spinen/laravel-clickup",
    "description": "SPINEN's Laravel Package for ClickUp.",
    "keywords": [
        "clickup",
        "client",
        "laravel",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        },
        {
            "name": "Stephen Finney",
            "email": "stephen.finney@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.4",
        "illuminate/container": "~5.7|~6|~7",
        "illuminate/database": "~5.7|~6|~7",
        "illuminate/encryption": "~5.7|~6|~7",
        "illuminate/http": "~5.7|~6|~7",
        "illuminate/routing": "~5.7|~6|~7",
        "nesbot/carbon": "~1.2|~2",
        "vlucas/phpdotenv": "^3.6|^4.0"
    },
    "require-dev": {
        "illuminate/auth": "~5.7|~6|~7",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4|^9.0",
        "psy/psysh": "^0.10",
        "symfony/thanks": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\ClickUp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\ClickUp\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\ClickUp\\Providers\\ClientServiceProvider",
                "Spinen\\ClickUp\\Providers\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
